#: modifytables.py:101
msgid "---"
msgstr "---"

#: modifytables.py:108
msgid "Information"
msgstr "Informações"

#: modifytables.py:170
msgid "HIDE cannot be combined with other actions"
msgstr "Não é possível combinar HIDE com outras ações"

#: modifytables.py:175
msgid "Row or column specification must be a sequence of one or more values"
msgstr "A especificação da linha ou coluna deve ser uma sequência de um ou mais valores"

#: modifytables.py:178
msgid "The rows dimension cannot be combined with resizing"
msgstr "A dimensão das linhas não pode ser combinada com o redimensionamento"

#: modifytables.py:182
msgid "Regular expression mode is not available with widths"
msgstr "O modo de expressão regular não está disponível com larguras"

#: modifytables.py:184
msgid "The number of column sizes specified is different from number of columns specified."
msgstr "O número de tamanhos de coluna especificado é diferente do número de colunas especificado."

#: modifytables.py:188
msgid "ROWLABELS and ROWLABELWIDTHS must be specified together"
msgstr "ROWLABELS e ROWLABELWIDTHS devem ser especificados juntos"

#: modifytables.py:193
msgid "The number of row label sizes specified is different from number of row labels specified."
msgstr "O número de tamanhos de rótulo de linha especificado é diferente do número de rótulos de linha especificado."

#: modifytables.py:196
msgid "SELECT is required unless ROWLABELS is specified"
msgstr "SELECT é obrigatório, a menos que ROWLABELS seja especificado"

#: modifytables.py:214
msgid "HIDE cannot be applied with columns = <<ALL>>"
msgstr "HIDE não pode ser aplicado com colunas = <<ALL>>"

#: modifytables.py:239
msgid "Set Background Color exception: %s %s %s"
msgstr "Definir exceção de cor de fundo: %s %s %s"

#: modifytables.py:362
msgid ""
"A specified row or column label number does not exist in a selected table.\n"
"It will be ignored.  Any column-specific width settings may be incorrect.\n"
"Label number: %s.  Table size: %s"
msgstr ""
"O número especificado de um rótulo de linha ou coluna não existe em uma tabela selecionada.\n"
"Ele será ignorado.  Alguma configuração de largura específica da coluna pode estar incorreta.\n"
"Número do rótulo: %s. Tamanho da tabela: %s"

#: modifytables.py:378
msgid "Table Labels: %s.  Dimensions: %s, %s"
msgstr "Rótulos da tabela: %s: Dimensões: %s, %s"

#: modifytables.py:415
msgid "APPLYTO expression is invalid: %s"
msgstr "A expressão APPLYTO é inválida: %s"

#: modifytables.py:457
msgid "Color specification must consist of three integers (R, G, and B values) between 0 and 255"
msgstr "A especificação da cor deve consistir em três números inteiros (valores R, G e B) entre 0 e 255"

#: modifytables.py:485
msgid "function reference %s not valid"
msgstr "a referência de função %s não é válida"

#: modifytables.py:493
msgid ""
"Invalid custom function signature.\n"
"Too few arguments: %s"
msgstr ""
"A assinatura personalizada de função é inválida.\n"
"Argumentos insuficientes: %s"

#: modifytables.py:514 modifytables.py:522
msgid "Invalid customfunction parameter expression: %s"
msgstr "A expressão do parâmetro customfunction é inválida: %s"

msgid "Messages"
msgstr "Mensagens"
